{
  "name": "spoiled-tomatillos-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node ./bin/www",
    "start-prod": "NODE_ENV=production node ./bin/www",
    "setup-dev-db": "cd db && ../node_modules/.bin/sequelize db:create && ../node_modules/.bin/sequelize db:migrate && ../node_modules/.bin/sequelize db:seed --seed 20180326071657-dev-seed && cd ..",
    "update-dev-db": "cd db && ../node_modules/.bin/sequelize db:migrate && cd ..",
    "cleanup-dev-db": "cd db && ../node_modules/.bin/sequelize db:drop && cd ..",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test": "node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha --exit"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "csv-parse": "^2.0.4",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "sequelize": "^4.33.4",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "sonar-scanner": "^3.0.3"
  }
}
